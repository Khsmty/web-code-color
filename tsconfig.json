{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["es2023", "dom"],
        "module": "commonjs",
        "rootDir": "./src",
        "declaration": true,
        "outDir": "./dist",
        "emitDeclarationOnly": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
